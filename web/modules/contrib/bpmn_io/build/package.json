{
  "name": "bpmn_io",
  "version": "1.0.0",
  "description": "BPMN-JS modeller for Drupal",
  "main": "app/index.js",
  "scripts": {
    "all": "run-s bundle",
    "dev": "run-s bundle:less watch:js",
    "start": "run-s bundle serve",
    "serve": "sirv public --dev",
    "bundle": "run-s bundle:*",
    "offbundle:less": "lessc src/app.less public/app.css",
    "bundle:js": "webpack -c webpack.config.js",
    "watch:js": "run-s \"bundle:js -- --watch\""
  },
  "license": "MIT",
  "dependencies": {
    "@bpmn-io/element-template-chooser": "^0.0.5",
    "@bpmn-io/element-templates-validator": "^0.8.1",
    "@bpmn-io/properties-panel": "^0.13.2",
    "bpmn-js": "^9.2.1",
    "bpmn-js-properties-panel": "^1.1.1",
    "camunda-bpmn-moddle": "^6.1.2",
    "@camunda/element-templates-json-schema": "^0.9.1",
    "diagram-js": "^8.5.0",
    "jquery": "^3.6.0",
    "min-dash": "^3.8.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^7.0.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "sirv-cli": "^1.0.14",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  }
}
