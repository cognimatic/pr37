        "drupal/security_review": "^2.0",
        "drupal/seo_checklist": "^5.1",
        "drupal/shs": "^2.0@RC",
        "drupal/simple_styleguide": "^1.8",
        "drupal/simplesamlphp_auth": "^3.2",
        "drupal/simplify": "^1.2",
