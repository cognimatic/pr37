        "drupal/media_entity_audio": "^3.0",
        "drupal/media_link_enhancements": "^1.0",
        "drupal/media_pdf_thumbnail": "^5.4",
        "drupal/memcache": "^2.3",
        "drupal/menu_block": "^1.10",
        "drupal/metatag": "^1.22",
        "drupal/migmag": "^1.8",
        "drupal/migrate_conditions": "^2.0",
        "drupal/migrate_devel": "^2.0@alpha",
        "drupal/migrate_drupal_d8": "^1.0",
        "drupal/migrate_file": "^2.1",
        "drupal/migrate_process_trim": "^2.0",
        "drupal/migrate_skip_on_404": "^1.1",
        "drupal/migrate_source_csv": "^3.5",
        "drupal/module_filter": "^3.2",
        "drupal/paragraphs_table": "^1.9",
        "drupal/prod_check": "^1.0@alpha",
        "drupal/rabbit_hole": "^1.0@beta",
