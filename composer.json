{
    "name": "localgovdrupal/localgov-project",
    "description": "Project template for the LocalGov Drupal distribution.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/localgovdrupal/localgov_project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.10",
        "cweagans/composer-patches": "^1.6",
        "defuse/php-encryption": "^2.3",
        "drupal/autocomplete_deluxe": "^2.0",
        "drupal/backup_migrate": "5.1.x-dev@dev",
        "drupal/block_class": "^2.0",
        "drupal/bpmn_io": "^1.0",
        "drupal/broken_link": "^3.1",
        "drupal/ckeditor_browser_context_menu": "^1.2",
        "drupal/cludo_search": "^1.0@beta",
        "drupal/coffee": "^1.2",
        "drupal/config_split": "^2.0@RC",
        "drupal/console": "~1.0",
        "drupal/content_moderation_notifications": "^3.4",
        "drupal/content_readability": "^1.0@alpha",
        "drupal/convert_bundles": "^1.0@alpha",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/csv_serialization": "^2.1",
        "drupal/db_maintenance": "^2.0",
        "drupal/eca": "^1.0",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/editoria11y": "^1.0",
        "drupal/environment_indicator": "^4.0",
        "drupal/extlink": "^1.6",
        "drupal/filefield_sources": "^1.0@alpha",
        "drupal/fpa": "^3.0",
        "drupal/honeypot": "^2.1",
        "drupal/httpswww": "^2.0",
        "drupal/image_effects": "^3.3",
        "drupal/image_resize_filter": "^1.1",
        "drupal/layout_builder_base": "1.x-dev@dev",
        "drupal/linkchecker": "^1.0@beta",
        "drupal/mail_login": "^2.6",
        "drupal/mailsystem": "^4.4",
        "drupal/media_directories": "^2.0",
        "drupal/media_link_enhancements": "^1.0",
        "drupal/memcache": "^2.3",
        "drupal/migmag": "^1.8",
        "drupal/migrate_conditions": "^2.0",
        "drupal/migrate_devel": "^2.0@alpha",
        "drupal/migrate_drupal_d8": "^1.0",
        "drupal/migrate_file": "^2.1",
        "drupal/migrate_process_trim": "^2.0",
        "drupal/migrate_skip_on_404": "^1.1",
        "drupal/migrate_source_csv": "^3.5",
        "drupal/module_filter": "^3.2",
        "drupal/paragraphs_table": "^1.9",
        "drupal/prod_check": "^1.0@alpha",
        "drupal/r4032login": "^2.2",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/rabbit_hole_href": "^2.0",
        "drupal/responsive_favicons": "^1.7",
        "drupal/scheduled_transitions": "^2.3@beta",
        "drupal/search404": "^2.0",
        "drupal/seckit": "^2.0",
        "drupal/seo_checklist": "^5.1",
        "drupal/shs": "^2.0@RC",
        "drupal/simple_styleguide": "^1.8",
        "drupal/simplesamlphp_auth": "^3.2",
        "drupal/simplify": "^1.2",
        "drupal/single_content_sync": "^1.3",
        "drupal/site_audit": "^3.0@RC",
        "drupal/smart_sql_idmap": "^1.1",
        "drupal/smtp": "^1.0",
        "drupal/svg_image": "^1.16",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/type_tray": "^1.1",
        "drupal/views_bulk_operations": "^4.1",
        "drupal/views_data_export": "^1.1",
        "drupal/webform": "^6.1",
        "drupal/webprofiler": "^9.0",
        "drupal/yoast_seo": "^1.7",
        "localgovdrupal/localgov": "^2.0",
        "localgovdrupal/localgov_multilingual": "1.x-dev",
        "localgovdrupal/localgov_os_places_geocoder_provider": "1.x-dev"
    },
    "require-dev": {
        "brianium/paratest": "^6.0",
        "drupal/bugherd": "^1.0@beta",
        "drupal/coder": "^8.3",
        "drupal/config_delete": "^1.17",
        "drupal/config_inspector": "^1.3",
        "drupal/core-dev": "^9.1",
        "drupal/delete_all": "^2.0",
        "drupal/devel": "^4.1",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entity_relationship_diagram": "1.x-dev@dev",
        "drupal/migrate_manifest": "^3.0",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/shield": "^1.4",
        "drupal/unused_modules": "^1.3",
        "drupal/userswitch": "^1.9",
        "kint-php/kint": "^4.1",
        "mglaman/phpstan-drupal": "^1.0",
        "phpspec/prophecy-phpunit": "^2",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "bin-dir": "bin/",
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "simplesamlphp/composer-module-installer": true,
            "drupal/console-extend-plugin": true
        }
    },
    "extra": {
        "enable-patching": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
				"[web-root]/sites/default/default.services.yml": false,
				"[web-root]/sites/default/default.settings.php": false,
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "patches": {
            "drupal/autocomplete_deluxe": {
                "Multiple reference terms fails 3053986": "https://www.drupal.org/files/issues/2021-05-25/3053986-6.patch"
            },
	        "drupal/cludo_search": {
	          "Cludo search autocomplete not working on block 3050293": "https://www.drupal.org/files/issues/2020-06-11/3050293-8.cludo_search.Cludo-search-autocomplete-not-working-on-block.patch"
	        },
			"drupal/core": {
				"Claro preprocessing of field multiple value forms 3099026": "https://www.drupal.org/files/issues/2022-10-20/3099026-42.patch",
				"Add type declaration html-escape decodeEntitties FormattableMarkup-placeholderEscape 3255637": "https://www.drupal.org/files/issues/2022-10-28/3255637-74.patch"
			},
			"drupal/gin": {
				"Custom toolbar logo lacks alt attribute 3293366": "https://git.drupalcode.org/project/gin/-/merge_requests/161.diff"
			},
			"drupal/migrate_drupal_d8": {
				"Fix bundle logic to select fields correctly 3134305": "https://www.drupal.org/files/issues/2021-07-16/3134305-fix-bundle-logic-5.patch"
			},
			"drupal/shs": {
				"Error saving null empty value 3241169": "https://www.drupal.org/files/issues/2022-02-28/shs-Error_saving_null_empty_value_after_2.0.0-rc1_update-3241169-36.patch"
			},
            "drupal/userswitch": {
                "Add an entity operation 3087885": "https://www.drupal.org/files/issues/2022-01-30/add-an-entity-operation-in-addition-to-user-list-3087885-29.patch"
            }
        },
        "enable-patching": true,
		 
        "drupal-core-project-message": {
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                                  </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the LocalGov Drupal codebase  </>",
                "<bg=blue;fg=white>                                                                  </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }

    }
}
