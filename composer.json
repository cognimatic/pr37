        "drupal/webprofiler": "^9.0",
        "drupal/yoast_seo": "^1.7",
        "localgovdrupal/localgov": "^2.0",
        "localgovdrupal/localgov_forms": "^1.0@beta",
        "localgovdrupal/localgov_moderngov": "1.x-dev",
        "localgovdrupal/localgov_multilingual": "1.x-dev",
        "localgovdrupal/localgov_os_places_geocoder_provider": "1.x-dev"
    },
    "require-dev": {
        "brianium/paratest": "^6.0",
        "drupal/bugherd": "^1.0@beta",
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^9.1",
        "drupal/devel": "^4.1",
        "drupal/devel_entity_updates": "^4.1",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/shield": "^1.4",
        "drupal/unused_modules": "^1.3",
        "drupal/upgrade_status": "^4.0@alpha",
        "drupal/userswitch": "^1.9",
        "kint-php/kint": "^5.0",
        "mglaman/phpstan-drupal": "^1.0",
        "phpspec/prophecy-phpunit": "^2",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "bin-dir": "bin/",
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "simplesamlphp/composer-module-installer": true,
            "drupal/console-extend-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "enable-patching": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
				"[web-root]/sites/default/default.services.yml": false,
				"[web-root]/sites/default/default.settings.php": false,
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "patches": {
            "drupal/auditfiles": {
                "NULL Count Error if no 'Managed Not Used' Files 3346307": "https://git.drupalcode.org/project/auditfiles/-/merge_requests/20.diff"
            },
            "drupal/autocomplete_deluxe": {
                "Multiple reference terms fails 3053986": "https://www.drupal.org/files/issues/2021-05-25/3053986-6.patch"
            },
            "drupal/civicccookiecontrol": {
                "No options in Text formats select box 3319073": "https://www.drupal.org/files/issues/2023-02-20/civicccoookiecontrol-fix-text-formats-3319073.patch"
            },
            "drupal/cludo_search": {
              "Cludo search autocomplete not working on block 3050293": "https://www.drupal.org/files/issues/2020-06-11/3050293-8.cludo_search.Cludo-search-autocomplete-not-working-on-block.patch"
            },
            "drupal/core": {
              "Claro preprocessing of field multiple value forms 3099026": "https://www.drupal.org/files/issues/2023-02-24/3099026-51.patch",
              "Deprecated function: pathinfo() on template_preprocess_image_style 3306131": "https://git.drupalcode.org/project/drupal/-/merge_requests/2680.diff",
              "New non translatable field on translatable content throws error 3025039": "https://www.drupal.org/files/issues/2022-09-30/3025039-MR1105-79.patch"
            },
            "drupal/layout_paragraphs": {
                "Overlay Widget Jump Stutter 3276258": "https://www.drupal.org/files/issues/2022-10-14/3276258-9.patch"
            },
            "drupal/migrate_drupal_d8": {
                    "Fix bundle logic to select fields correctly 3134305": "https://www.drupal.org/files/issues/2021-07-16/3134305-fix-bundle-logic-5.patch"
            },
            "drupal/search_api_stats": {
                "Empty search returns error 3325990": "https://www.drupal.org/files/issues/2022-12-08/search_api_stats-empty-search-returns-error-3325990-2.patch"
            },
            "drupal/security_review": {
                "Base table or view not found 3345844": "https://git.drupalcode.org/project/security_review/-/merge_requests/37.diff"
            },
            "drupal/shs": {
                    "Error saving null empty value 3241169": "https://www.drupal.org/files/issues/2022-02-28/shs-Error_saving_null_empty_value_after_2.0.0-rc1_update-3241169-36.patch"
            },
            "drupal/userswitch": {
                "Add an entity operation 3087885": "https://www.drupal.org/files/issues/2022-01-30/add-an-entity-operation-in-addition-to-user-list-3087885-29.patch"
            }
        },
		 
        "drupal-core-project-message": {
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                                  </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the LocalGov Drupal codebase  </>",
                "<bg=blue;fg=white>                                                                  </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }

    }
}
